    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.3",
    "socket.io-client": "^4.8.1",
    "zod": "^3.25.67"
